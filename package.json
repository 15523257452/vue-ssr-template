{
  "name": "vue-ssr-template",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "client": "webpack-dev-server --mode development --config config/webpack.client.js",
    "build:client": "webpack --mode=production --config config/webpack.client.js",
    "build:server": "webpack --mode=production --config config/webpack.server.js",
    "build": "npm run build:client && npm run build:server",
    "dev": "npm run client && node config/dev.server.js ENV=development",
    "prod": "node config/prod.server.js ENV=production"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "memory-fs": "^0.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.0",
    "typescript": "^3.9.6",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "vue": "latest",
    "vue-loader": "^15.9.3",
    "vue-router": "^3.3.4",
    "vue-server-renderer": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9",
    "webpack-node-externals": "^2.3.0"
  },
  "devDependencies": {
    "webpack-cli": "^3.3.12"
  }
}
